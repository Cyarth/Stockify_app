{
    "version": 2,
    "builds": [
      {
        "src": "stock-app-frontend/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "stock-app-backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/stock-app-backend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/stock-app-frontend/$1"
      }
    ]
  }
  